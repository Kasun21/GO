<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDFDFD"
    tools:context=".done">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <Button
        android:id="@+id/Methods2"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:backgroundTint="#000000"
        android:text="@string/VIEW_USER_PROFILE"
        android:textColor="#F8F5F5"
        app:cornerRadius="20dp"
        app:iconTint="#EFE1C6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="401dp"
        android:layout_height="380dp"
        android:layout_marginTop="168dp"
        app:layout_constraintBottom_toBottomOf="@+id/Methods2"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/paymentt" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="378dp"
        android:layout_height="93dp"
        android:text="Payment Successfull !"
        android:textColor="#000000"
        android:textColorHighlight="#070707"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="125dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="60dp"
        android:layout_height="62dp"
        app:srcCompat="@drawable/successfull"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="63dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="237dp"
        android:layout_height="28dp"
        android:text="Payment of Rs.1600.00 Confimed"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="168dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="83dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:srcCompat="@drawable/threedots"
        tools:layout_editor_absoluteY="-7dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="40dp"
        android:layout_height="32dp"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Payment Successfull"
        android:textColor="#070707"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="10dp" />

    <View
        android:id="@+id/divider3"
        android:layout_width="350dp"
        android:layout_height="3dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:background="@color/black"
        android:backgroundTint="#000000"
        app:layout_constraintBottom_toTopOf="@+id/Methods2"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/divider5"
        android:layout_width="350dp"
        android:layout_height="3dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:background="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />
</androidx.constraintlayout.widget.ConstraintLayout>